{
  "name": "positive-vibes-gate",
  "version": "1.0.0",
  "description": "GitHub Action that blocks deployments if your team lacks positive emoji vibes! 🎉✨",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build dist/index.js -o dist --source-map --license licenses.txt",
    "package": "pnpm run build",
    "precommit": "pnpm run build && git add dist/",
    "test": "echo \"No tests yet\" && exit 0",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeanding/positive-vibes-gate.git"
  },
  "keywords": [
    "github-action",
    "deployment",
    "positive-vibes",
    "emojis",
    "fun",
    "hackathon",
    "typescript"
  ],
  "author": "@codeanding",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@octokit/rest": "^20.1.1",
    "@vercel/ncc": "^0.38.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.15.0"
}
